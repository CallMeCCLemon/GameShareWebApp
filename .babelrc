{
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "components": "./components",
          "containers": "./containers",
          "queries": "./graphql/queries"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "presets": [
        "next/babel"
      ]
    },
    "production": {
      "presets": [
        "next/babel"
      ]
    },
    "test": {
      "plugins": [
        "require-context-hook"
      ],
      "presets": [
        "react",
        "env",
        "stage-0"
      ]
    }
  }
}